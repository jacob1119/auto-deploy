  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch craveuai/api-server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/craveu/api-server/main.go",
      "args": [
        "-e",
        "${workspaceFolder}/cmd/craveu/api-server/.env"
      ]
    },
    {
      "name": "Launch craveuai/timecapsule",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/craveu/timecapsule-consumer/main.go",
      "args": [
        "-e",
        "${workspaceFolder}/cmd/craveu/api-server/.env"
      ]
    },
  ]
}